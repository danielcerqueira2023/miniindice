{
  "expo": {
    "name": "Dashboard WIN",
    "slug": "dashboard-win",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dashboardwin",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#121214",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dualite.dashboardwin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121214"
      },
      "package": "com.dualite.dashboardwin"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "960585f7-e3fa-47ee-aceb-d86550e1e344"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/960585f7-e3fa-47ee-aceb-d86550e1e344"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
